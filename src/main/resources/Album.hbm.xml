<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

 <hibernate-mapping>
 	<class name="com.quintor.study_case.entities.Album" table="albums" lazy="false">
 		<id name="albumId" column="album_id" type="int">
 			<generator class="increment"/>
 		</id>
 		<property name="albumName" column="album_name" type="string"/>
 		<set name="songs" table="songs" inverse="true" fetch="select">
 			<key>
 				<column name="album_id" not-null="true"/>
 			</key>
 			<one-to-many class="com.quintor.study_case.entities.Song"/>
 		</set>
 		<many-to-one name="artist" class="com.quintor.study_case.entities.Artist" fetch="select">
 			<column name="artist_id" not-null="true"/>
 		</many-to-one>
 	</class>
 </hibernate-mapping>